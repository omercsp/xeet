{
	"tests": [
		{
			"name": "001_out_only",
			"command": "echo for stdout",
			"groups": ["stdout", "passing"]
		},
		{
			"name": "002_err_only",
			"shell": true,
			"command": "echo for stderr 1>&2",
			"groups": ["stderr", "passing"]
		},
		{
			"name": "003_out_and_err",
			"shell": true,
			"command": "echo for stdout; echo for stderr 1>&2",
			"groups": ["stderr", "passing", "stdout"]
		},
		{
			"name": "004_no_output",
			"command": "true",
			"groups": ["passing"]
		},
		{
			"name": "007_unified_stdout_stderr",
			"shell": true,
			"output_behavior": "unify",
			"command": "echo for stdout; echo for stderr 1>&2",
			"groups": ["passing"]
		},
		{
			"name": "050_ignore_stderr",
			"shell": true,
			"command": "echo for stdout; echo for stderr 1>&2",
			"compare_output": "stdout",
			"groups": ["passing"]
		},
		{
			"name": "051_ignore_stdout",
			"shell": true,
			"command": "echo for stdout; echo for stderr 1>&2",
			"compare_output": "stderr",
			"groups": ["passing"]
		},
		{
			"name": "100_missing_stdout",
			"command": "true",
			"groups": ["failing"]
		},
		{
			"name": "101_missing_stderr",
			"command": "true",
			"groups": ["failing"]
		},
		{
			"name": "102_unexpected_stdout",
			"command": "echo unexpected stdout",
			"groups": ["failing"]
		},
		{
			"name": "103_unexpected_stderr",
			"shell": true,
			"command": "echo unexpected stderr 1>&2",
			"groups": ["failing"]
		}
	]
}
